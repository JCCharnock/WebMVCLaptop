[
  {
    "id":  "1",
    "name": "Southern Style Green Beans",
    "ingredients": [
      {
        "quantity": "4",
        "name": "slices bacon, diced"
      },
      {
        "quantity": "1 lbs",
        "name": "green beans ends snapped off and longer beans snapped in half"
      },
      {
        "quantity": "2 cups",
        "name": "chicken broth"
      },
      {
        "quantity": "2 cups",
        "name": "water"
      },
      {
        "quantity": "1 tsp",
        "name": "seasoned salt"
      },
      {
        "quantity": "1/2 tsp",
        "name": "black pepper"
      },
      {
        "quantity": "1/2 tsp",
        "name": "garlic powder"
      },
      {
        "quantity": "1/4 tsp",
        "name": "red pepper flakes"
      },
      {
        "quantity": "1 tbsp",
        "name": "butter"
      }
    ],
    "steps": [
      {
        "order": "1",
        "text": "Brown and crisp bacon in a large pot. Remove bacon from pot and reserve"
      },
      {
        "order": "2",
        "text": "Add green beans to pot along with all remaining ingredients, except butter"
      },
      {
        "order": "3",
        "text": "Bring to a boil and then turn heat to medium-low. Cover and simmer for 1-2 hours, stirring occasionally"
      },
      {
        "order": "4",
        "text": "Drain beans and add butter if using. Check beans for seasoning and add extra salt and pepper to taste. I like lots of black pepper. Sprinkle with bacon and toss to distribute the bacon and butter"
      }
    ],
    "imageURL": "http://img.sndimg.com/food/image/upload/w_266/v1/img/recipes/27/20/8/picVfzLZo.jpg",
    "originalURL": "http://www.food.com/recipe/to-die-for-crock-pot-roast-27208"
  },
  {
    "id": "2",
    "name": "Mexican Black Beans",
    "ingredients": [
      {
        "quantity": "1 tsp",
        "name": "olive oil"
      },
      {
        "quantity": "1/2 cup",
        "name": "chopped onions"
      },
      {
        "quantity": "3 cloves",
        "name": "minced garlic"
      },
      {
        "quantity": "1 16oz can",
        "name": "undrained black beans"
      },
      {
        "quantity": "1/4 cup",
        "name": "chopped cilantro"
      },
      {
        "quantity": "1 tsp",
        "name": "cumin"
      },
      {
        "quantity": "1/2 tsp",
        "name": "salt"
      }
    ],
    "steps": [
      {
        "order": "1",
        "text": "In a small sauce pan, heat the olive oil over medium heat. Add the onions and garlic and cook for 3-4 minutes, just until the onions begin to soften."
      },
      {
        "order": "2",
        "text": "Add the undrained black beans, cilantro, cumin, and salt. Stir well and reduce heat to medium low. Allow to cook for 15-20 minutes, stirring occasionally. Taste and adjust seasonings to your liking"
      },
      {
        "order": "3",
        "text": "Serve sprinkled with more chopped cilantro and cotija cheese if desired"
      },
      {
        "order": "4",
        "text": "Cook on low for 7-9 hours."
      }
    ],
    "imageURL": "http://img.sndimg.com/food/image/upload/w_266/v1/img/recipes/27/20/8/picVfzLZo.jpg",
    "originalURL": "http://www.food.com/recipe/to-die-for-crock-pot-roast-27208"
  },
  {
    "id": "3",
    "name": "Mussels",
    "ingredients": [
      {
        "quantity": "4 lbs",
        "name": "live mussels"
      },
      {
        "quantity": "2 cups",
        "name": "dry white wine"
      },
      {
        "quantity": "4",
        "name": "large shallots, finely chopped"
      },
      {
        "quantity": "4",
        "name": "garlic cloves, finely chopped"
      },
      {
        "quantity": "1/2 tsp",
        "name": "salt"
      },
      {
        "quantity": "1/3 cup",
        "name": "fresh mixed herbs (such as flat-leaf parsley, chervil, or basil, chopped)"
      },
      {
        "quantity": "6 tbsp",
        "name": "tablespoons butter, cut into pieces"
      }
    ],
    "steps": [
      {
        "order": "1",
        "text": "Rinse and scrub mussels under cold water"
      },
      {
        "order": "2",
        "text": "Using your fingers or paring knife, remove beards"
      },
      {
        "order": "3",
        "text": "in a large stockpot set over medium heat, combine wine, shallots, garlic, and salt"
      },
      {
        "order": "4",
        "text": "Simmer 5 minutes."
      }
    ],
    "imageURL": "http://img.sndimg.com/food/image/upload/w_266/v1/img/recipes/27/20/8/picVfzLZo.jpg",
    "originalURL": "http://www.food.com/recipe/to-die-for-crock-pot-roast-27208"
  }
]
